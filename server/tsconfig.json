{
    "compilerOptions": {
      "module": "commonjs",
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false, 
      "noImplicitThis": false,
      "sourceMap": true,
      "baseUrl": ".",
      "types": [
        "webpack-env"
      ],
      "paths": {
        "@/*": [
            "node_modules/",
            "types/*"
        ]
      }
    },
    "exclude": [
        "node_modules"
    ]
  }